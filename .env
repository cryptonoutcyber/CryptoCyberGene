# Solana RPC Configuration (use your own API key)
RPC_URL=https://api.mainnet-beta.solana.com
HELIUS_API_KEY=your_helius_api_key_here
# Use Helius RPC if API key is provided, otherwise fallback to default
# RPC_URL will be dynamically set in code based on HELIUS_API_KEY availability

# Relayer Configuration (replace with your actual values)
RELAYER_URL=https://your-relayer-domain/relay/sendRawTransaction
RELAYER_PUBKEY=your_relayer_fee_payer_pubkey_here
RELAYER_API_KEY=your_relayer_api_key_here

# Wallet Configuration (replace with your actual values)
TREASURY_PUBKEY=your_treasury_pubkey_here
TARGET_WALLET_ADDRESS=your_target_wallet_address_here

# Authority and Security Settings
AUTHORITY_MODE=null
DRY_RUN=true
